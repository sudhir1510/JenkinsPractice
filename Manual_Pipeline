def readProp;

pipeline {
    agent none
	
	stages {
        
		stage ('prepare') {
			checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/sudhir1510/JenkinsPractice.git']]])
		}
		stage('Build') {
			steps {
				echo 'hi sudh'
			}
			
        }
    }
}
